<?xml version="1.0"?>
<opencv_storage>
<Settings>
   <!-- Whether to adapt calibration images -->
  <AdaptImage>0</AdaptImage>  
  
  <!-- Number of INNER corners per a item row and column. (square, circle) -->
  <BoardSize_Width> 9</BoardSize_Width>
  <BoardSize_Height>14</BoardSize_Height>
  
  <!-- The name of the input directory of Calibration Images-->
  <InputDirectory>"CALIB"</InputDirectory>
  
  <!-- Whether to show calibration images -->
  <ShowCalibration>1</ShowCalibration>
  
  <!-- If true (non-zero) we show after calibration the undistorted images.-->
  <Show_UndistortedImage>1</Show_UndistortedImage>
  
  <!-- The size of a square in some user defined metric system (pixel, millimeter)-->
  <Square_Size>14.85</Square_Size>
  
  <!-- The name of the output file. -->
  <Write_OutputFileName>"PRE_PROCESS/Calibration_results.yml"</Write_OutputFileName>
  
  <!-- If true (non-zero) we write to the output file the reprojection errors per view-->
  <WritePerView>0</WritePerView>

</Settings>
</opencv_storage>
